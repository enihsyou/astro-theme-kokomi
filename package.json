{
  "name": "astro-theme-kokomi",
  "description": "An Astro theme template for building knowledge-sharing blogs.",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "build:docker": "docker build -t astro-theme-kokomi .",
    "preview": "astro preview",
    "clean": "rm -rf .astro dist",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "link:astro-kokomi": "bun --cwd=packages/astro-kokomi link; bun link astro-kokomi",
    "link:astro-kokomi:help": "# link packages/astro-kokomi for local development"
  },
  "dependencies": {
    "astro": "^5.7.10"
  },
  "devDependencies": {
    "@enihsyou/prettier-config": "^0.0.2",
    "@eslint/js": "^9.26.0",
    "@unocss/preset-wind4": "^66.1.1",
    "eslint": "^9.26.0",
    "eslint-plugin-astro": "^1.3.1",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "typescript-eslint": "^8.32.0",
    "unocss": "^66.1.1"
  }
}
